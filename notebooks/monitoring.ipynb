    stat, p_val, drifted = detect_drift(reference_data, production_week1, col)
    week1_results.append({
        'Feature': col,
        'KS_Statistic': f'{stat:.4f}',
        'P_Value': f'{p_val:.4f}',
        'Drift?': 'ğŸš¨ YES' if drifted else 'âœ… NO'
    })

week1_df = pd.DataFrame(week1_results)
